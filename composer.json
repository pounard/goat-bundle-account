{
    "name" : "makinacorpus/goat-bundle-account",
    "type": "library",
    "description": "Goat user account management bundle",
    "homepage": "http://github.com/makinacorpus/goat-account-bundle",
    "license": "GPL-2",
    "authors": [
        {
            "name" : "Pierre RINEAU",
            "email" : "pierre.rineau@processus.org"
        }
    ],
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "autoload" : {
        "exclude-from-classmap": ["/Tests/"],
        "psr-4" : {
            "Goat\\AccountBundle\\" : "."
        }
    },
    "require" : {
        "ext-mcrypt" : "*",
        "makinacorpus/goat" : "*",
        "makinacorpus/goat-bundle" : "*",
        "php" : ">=7.0",
        "symfony/framework-bundle": "^3.2 || ^4",
        "symfony/security": "^3.2 || ^4"
    },
    "require-dev" : {
        "phpunit/phpunit" : "5.*"
    }
}
